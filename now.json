{
  "version": 2,
  "name": "konsilos",
  "env": {
    "IS_NOW": "true",
    "STYLIS_SHOULD_PREFIX": "false"
  },
  "routes": [
    {
      "src": "^/service-worker.js$",
      "dest": "/_next/static/service-worker.js",
      "headers": {
        "cache-control": "public, max-age=43200, immutable",
        "Service-Worker-Allowed": "/"
      }
    },
    {
      "src": "/graphql",
      "dest": "https://___YOUR__GRAPHQL___"
    },
    {
      "src": "/manifest.json",
      "dest": "/_next/static/manifest.json",
      "headers": {
        "cache-control": "max-age=43200, s-maxage=86400"
      }
    },
    {
      "src": "/sitemap.xml",
      "dest": "/_next/static/sitemap.xml",
      "headers": {
        "cache-control": "max-age=43200, s-maxage=86400"
      }
    },
    {
      "src": "/robots.txt",
      "dest": "/_next/static/robots.txt",
      "headers": {
        "cache-control": "max-age=43200, s-maxage=86400"
      }
    },
    {
      "src": "/favicon.ico",
      "dest": "https://___YOUR__FAVICON___",
      "headers": {
        "cache-control": "max-age=43200, s-maxage=86400"
      }
    },
    {
      "src": "/favicon.png",
      "dest": "https://___YOUR__FAVICON___",
      "headers": {
        "cache-control": "max-age=43200, s-maxage=86400"
      }
    },
    {
      "src": "/static/(.*)",
      "dest": "/static/$1",
      "headers": {
        "cache-control": "max-age=43200, s-maxage=86400"
      }
    },
    {
      "src": "/ui/(.*)",
      "dest": "storybook/$1",
      "headers": {
        "cache-control": "max-age=43200, s-maxage=86400"
      }
    },
    {
      "src": "/(.*)",
      "dest": "/$1",
      "headers": {
        "cache-control": "max-age=43200, s-maxage=86400"
      }
    }
  ],
  "builds": [
    {
      "src": "package.json",
      "use": "@now/next"
    }
  ]
}
